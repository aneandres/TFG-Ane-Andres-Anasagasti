int index=0;
int ciclos_epilepsia = 3; // el ataque de epilepsia durará 3*10 = 30 segundos
int ciclo_i = 0;

int tsamp_ms = 5; // 4000*5/1000 = 20 segundos de señal en total
int resolucion = 12; // depende del convertidor de la placa

void setup() {
  Serial.begin(115200);
  while (!Serial) {
    ; //Wait for serial port to connect. Needed for native USB only.
  }
  pinMode(DAC,OUTPUT);
  pinMode(A1,INPUT);

  analogWriteResolution(resolucion);
  Serial.println("Leemos la salida analógica...");
}

void loop() {
  if (millis()%tsamp_ms==0) // cada tsamp milisegundos cambiamos de estado
  {
    int valor_ECG = ECG_mixto[index];
    index++;
    analogWrite(DAC,valor_ECG);
    if (index==4000){
      if (ciclo_i==ciclos_epilepsia) {
        ciclo_i = 0;
        index = 0;
      } else { //generamos una convulsión durante (10*ciclos_epilepsia) segundos
        ciclo_i++;
        index = 2000;
      }
    }
    delay(10);
  }
  Serial.println(analogRead(A1)); //leemos la señal en Serial Plotter
}